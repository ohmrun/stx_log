{
  "build" : {
    "-D" : ["no-deprecation-warnings"],
    "-lib" : [
      "tink_core","hx3compat","stx_pico","tink_core","stx_pico","stx_nano",
      "stx_assert","stx_fp","stx_fn","stx_test","console.hx","hre","polygonal-printf"
    ],
    "-cp"  : ["src/main/haxe"],
    "main" : {
      "-main"     : "Main",
      "hashlink"  : {
        "-hl"     : "build/test/hl/main.hl" 
      },
      "neko" : {
        "-neko" : "build/neko/main.n"      
      },
      "interp" : {
        "--interp" : true
      }
    }
  },
  "run" : {
    "hashlink"  : [["hl","build/test/hl/main.hl"]]
  }
}
